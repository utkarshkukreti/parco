{
  "name": "parco",
  "version": "0.6.0",
  "description": "A hyper-optimized 1kb library to build fully type-safe parsers in TypeScript.",
  "keywords": [
    "combinator",
    "ll",
    "parse",
    "parsec",
    "parser combinator",
    "parser",
    "parsing",
    "regex",
    "regexp",
    "string",
    "typesafe",
    "typescript"
  ],
  "homepage": "https://github.com/utkarshkukreti/parco",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/parco.umd.production.min.js",
  "unpkg": "dist/parco.umd.production.min.js",
  "module": "dist/parco.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "prepare": "yarn build",
    "format": "prettier --write .",
    "bench:json": "tsc -P tsconfig.cjs.json && node dist/benchmarks/json.js",
    "lint": "tsdx lint examples src tests"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "benchmark": "^2.1.4",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^11.0.2",
    "json-test-suite": "^0.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-sorted": "^2.0.0",
    "snowpack": "^3.0.11",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.13.0",
    "**/@typescript-eslint/parser": "^4.13.0",
    "**/jest": "^26.6.3",
    "**/ts-jest": "^26.4.4",
    "**/typescript": "^4.1.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
