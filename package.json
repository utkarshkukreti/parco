{
  "name": "parco",
  "version": "0.15.0",
  "description": "A hyper-optimized 1kb library to build fully type-safe parsers in TypeScript.",
  "keywords": [
    "combinator",
    "ll",
    "parse",
    "parsec",
    "parser combinator",
    "parser",
    "parsing",
    "regex",
    "regexp",
    "string",
    "typesafe",
    "typescript"
  ],
  "homepage": "https://github.com/utkarshkukreti/parco",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "tsc && vitest",
    "prepare": "pnpm build",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "bench:json": "vite-node benchmarks/json.ts"
  },
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@types/node": "22.19.3",
    "json-test-suite": "0.1.0",
    "prettier": "3.7.4",
    "tinybench": "4.1.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vite-node": "3.2.4",
    "vitest": "3.2.4"
  }
}
